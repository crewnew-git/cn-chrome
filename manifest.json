{
    "name": "CrewNew Check",
    "description": "Check if the URL exists in CrewNew DB",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}
