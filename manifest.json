{
  "name": "CrewNew Check",
  "description": "Check if the URL exists in CrewNew DB",
  "version": "1.1",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": [
        "https://*.linkedin.com/*",
        "https://*.upwork.com/*",
        "https://*.freelancer.com/*",
        "https://*.peopleperhour.com/*",
        "https://*.angel.co/*",
        "https://*.remotehub.com/*",
        "https://*.stackoverflow.com/*",
        "https://*.github.com/*"
      ],
      "js": ["script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "favicon.png"
  },
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  }
}
