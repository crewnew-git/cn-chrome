{
  "name": "CrewNew Check",
  "description": "Check if the URL exists in CrewNew DB",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/*", "https://*.upwork.com/*"],
      "js": ["script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "favicon.png"
  },
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  }
}
